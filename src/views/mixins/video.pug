mixin video(video)
  a(href=`/videos/${String(video.id)}`).video-mixin
    div.video-mixin__thumb(style=`background-image:url(${idDev? "/" : ""}${video.thumbUrl})`)
    div.video-mixin__data
      span.video-mixin__title=video.title
      div.video-mixin__meta
        span #{video.owner.name} • 
        span #{video.meta.views} 회